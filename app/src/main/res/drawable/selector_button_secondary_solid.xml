<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="false">
    <shape>
        <solid android:color="@color/secondaryColor"/>
        <corners android:radius="@dimen/standard_button_radius"/>
        <padding android:bottom="@dimen/android_standard_margin" android:left="@dimen/android_standard_margin" android:right="@dimen/android_standard_margin" android:top="@dimen/android_standard_margin" />

    </shape>

</item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/secondaryColorVariant"/>
            <corners android:radius="@dimen/standard_button_radius"/>
            <padding android:bottom="@dimen/android_standard_margin" android:left="@dimen/android_standard_margin" android:right="@dimen/android_standard_margin" android:top="@dimen/android_standard_margin" />

        </shape>

    </item>
</selector>